[Unit]
Description=Independent Test of Cluster Health
After=syslog.target network.target

[Service]
SuccessExitStatus=143

User=ec2-user

Type=simple

WorkingDirectory=/home/ec2-user/ITCH
ExecStart=/usr/bin/java -cp target/itch-1.0.1.jar com.sorintlab.itch.Itch
ExecStop=/bin/kill -15 $MAINPID

Restart=always

[Install]
WantedBy=multi-user.target